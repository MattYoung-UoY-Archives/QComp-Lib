package mjy.qcom.maths;

public class CompVec extends CompMat{
	
	public CompVec(ComplexNum[] vector) {
		super(vector);
	}
	
	public ComplexNum getI(int index) {
		return super.getCjk(index, 0);
	}
	
	public int getDimensionality() {
		int[] temp = super.getDimensions();
		return temp[1] + 1;
	}
	
	@Override
	public String toString() {
		String result = "[";
		for(int i = 0; i < this.getDimensionality(); i++) {
			result += "(" + this.getDimensionality() + ")";
			if(i < this.getDimensionality() - 1) result += ", ";
		}
		result += "]";
		return result;
	}
	
}
